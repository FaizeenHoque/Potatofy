{
  "manifest_version": 3,
  "name": "Potato Vision",
  "version": "1.0",
  "description": "Replaces all images with potatoes",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["potato.jpg"],
      "matches": ["<all_urls>"]
    }
  ]
}